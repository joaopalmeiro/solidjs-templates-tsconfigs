  },
  "files": [
    "bare/",
    "ts-minimal/"
  ],
  "scripts": {
    "format": "run-s format:pkgJson format:code",
    "format:code": "dprint fmt",
    "format:pkgJson": "sort-package-json",
    "lint": "run-s lint:pkgJson lint:filePaths lint:code",
    "lint:code": "dprint check",
    "lint:filePaths": "dprint output-file-paths",
    "lint:pkgJson": "sort-package-json --check"
  },
  "devDependencies": {
    "dprint": "0.45.0",
    "npm-run-all2": "6.1.2",
    "sort-package-json": "2.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

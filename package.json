{
  "name": "solidjs-templates-tsconfigs",
  "version": "0.1.0",
  "description": "TSConfig files for projects created from the solidjs/templates and solidjs/solid-start repos.",
  "homepage": "https://github.com/joaopalmeiro/solidjs-templates-tsconfigs",
  "bugs": {
    "url": "https://github.com/joaopalmeiro/solidjs-templates-tsconfigs/issues"
  },
  "repository": {
    "type": "git",
